<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="KeyboardOverlayHeader">
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">0.0dip</item>
        <item name="android:layout_weight">200.0</item>
    </style>
    <style name="OptInFirstPageIcon">
        <item name="android:layout_marginTop">@dimen/opt_in_first_page_icon_margin_top</item>
        <item name="android:layout_marginBottom">@dimen/opt_in_first_page_icon_margin_bottom</item>
        <item name="android:layout_marginEnd">@dimen/opt_in_first_page_positive_button_margin_end</item>
    </style>
    <style name="PermissionNoticeContentText">
        <item name="android:textSize">@dimen/permission_notice_content_text_size</item>
        <item name="android:textColor">#8a000000</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:paddingBottom">@dimen/permission_notice_content_text_padding_bottom</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">false</item>
        <item name="android:lineSpacingExtra">@dimen/permission_notice_content_text_line_space_extra</item>
    </style>
    <style name="PrivacyNotice" parent="@style/KeyboardOverlay">
        <item name="android:orientation">horizontal</item>
    </style>
    <style name="PrivacyNoticeAppImage">
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:scaleType">centerInside</item>
        <item name="android:layout_weight">275.0</item>
    </style>
    <style name="PrivacyNoticeContentWrapper">
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">725.0</item>
    </style>
    <style name="SoftKeyCandidateBarShowMoreCandidate">
        <item name="android:layout_gravity">right</item>
        <item name="android:background">?BgKeyRectPressedSelector</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minWidth">@dimen/softkey_show_more_candidates_min_width</item>
    </style>
    <style name="TranslateBoardLanguageBarFrame">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="TranslateBoardLanguageSelector">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:minWidth">100.0dip</item>
    </style>
    <style name="Widget.Design.TabLayout" parent="@style/Base.Widget.Design.TabLayout">
        <item name="tabMode">fixed</item>
        <item name="tabGravity">center</item>
    </style>
</resources>
