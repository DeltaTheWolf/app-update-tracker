<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/music_bottom_sheet_scrubber_bar_corner_radius" />
            <solid android:color="@color/music_bottom_sheet_secondary_color" />
            <size android:height="3.0dip" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape android:shape="rectangle">
                <corners android:radius="@dimen/music_bottom_sheet_scrubber_bar_corner_radius" />
                <solid android:color="@color/music_bottom_sheet_primary_color" />
                <size android:height="3.0dip" />
            </shape>
        </clip>
    </item>
</layer-list>